    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "scripts": {
        "drupal-l10n": "DrupalComposer\\DrupalL10n\\Plugin::download",
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "post-create-project-cmd": [
            "bash ./scripts/init.sh"
        ],
        "post-install-cmd": [
            "@drupal-scaffold",
            "bash ./scripts/update-libraries.sh"
        ],
        "post-update-cmd": [
            "@drupal-scaffold",
            "bash ./scripts/update-libraries.sh"
        ]
    },
    "extra": {
        "installer-paths": {
            "www/core": [
                "type:drupal-core"
            ],
            "www/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "www/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "www/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "www/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "www/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "www/libraries/{$name}": [
                "type:drupal-library"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "drupal-scaffold": {
            "excludes": [
                "update.php",
                "web.config",
                "sites/development.services.yml",
                "sites/example.settings.local.php"
            ]
        },
        "drupal-l10n": {
            "destination": "translations/contrib",
            "languages": [
                "fr"
            ]
        },
        "patches": {
            "drupal/bootstrap": {
                "Updating to Drupal core 8.4.0-beta1 results in not working dropdown menu": "https://www.drupal.org/files/issues/updating_to_drupal_core-2903656-3.patch"
            },
            "drupal/flag": {
                "[regression] restore 'All' option in flag Views filter handler": "https://www.drupal.org/files/issues/flag-all_option-2813503-3.patch"
            },
            "drupal/leaflet": {
                "Error in leaflet_process_geofield() when twig debug is active.": "https://www.drupal.org/files/issues/leaflet-error_when_debug_active-2783843-2.patch",
                "No field recognized in 'Leaflet map' views style settings in 8.3.0-alpha1": "https://www.drupal.org/files/issues/leaflet_map-no_field_recognized-2852506-5.patch"
            },
            "drupal/media_entity_browser": {
                "The browser entity should list the associated view as a config dependency.": "https://www.drupal.org/files/issues/meb_add_config-2850574-2.patch"
            },
            "drupal/menu_attributes": {
                "Compatibility with fontawesome menu icons": "https://www.drupal.org/files/issues/menu_attributes-compatibility_fontawesome_menu_icons-2896652-2.patch"
            },
            "drupal/redis": {
                "Add status page and some statistics": "https://www.drupal.org/files/issues/redis-statistics-7.patch"
            },
            "drupal/views_infinite_scroll": {
                "Views Infinite Scroll does not work with EVA views": "https://www.drupal.org/files/issues/EVA_views_now_support_infinite_scroll.patch"
            }
        }
    }
}
