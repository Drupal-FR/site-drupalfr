        "drupal/media_entity_actions": "1.0.0-alpha2",
        "drupal/media_entity_browser": "2.0.0-alpha3",
        "drupal/menu_attributes": "1.x-dev#b3168e1e390d5d80c9c984ae8436f8b722e936c2",
        "drupal/metatag": "1.15.0",
        "drupal/migrate_plus": "4.2.0",
        "drupal/migrate_source_csv": "2.2.0",
        "drupal/migrate_tools": "4.5.0",
        "drupal/paragraphs": "1.12.0",
        "drupal/pathauto": "1.8.0",
        "drupal/profile": "1.1.0",
        "drupal/publication_date": "2.0.0-beta2",
        "drupal/purge": "3.0.0",
        "drupal/r4032login": "1.1.0",
        "drupal/recurring_period": "1.0.0-alpha5",
        "drupal/redirect": "1.6.0",
        "drupal/redis": "1.4.0",
        "drupal/search_api": "1.17.0",
        "drupal/smart_trim": "1.3.0",
        "drupal/state_machine": "1.0.0",
        "drupal/swiftmailer": "1.0.0-beta2",
        "drupal/token": "1.7.0",
        "drupal/varnish_purge": "1.12.0",
        "drupal/video_embed_field": "2.4.0",
        "drupal/views_block_filter_block": "1.0.0",
        "drupal/views_bulk_edit": "2.4.0",
        "drupal/views_bulk_operations": "3.9.0",
        "drupal/views_custom_cache_tag": "1.2.0",
        "drupal/views_data_export": "1.0.0",
        "drupal/views_field_view": "1.0.0-beta3",
        "drupal/views_infinite_scroll": "1.7.0",
        "drupal/webform": "5.21.0",
        "drupal/zurb_foundation": "6.0.0-alpha4",
        "drush/drush": "9.7.2",
        "oomphinc/composer-installers-extender": "^2",
        "wikimedia/composer-merge-plugin": "dev-feature/composer-v2",
        "zaporylie/composer-drupal-optimizations": "^1.1"
    },
    "require-dev": {
        "anam/phantomjs-linux-x86-binary": "~2.1.1",
        "bower-asset/d3": "3.5.*",
        "bower-asset/highlightjs": "9.12.*",
        "drupal/config_inspector": "1.*",
        "drupal/console": "1.*",
        "drupal/core-dev": "8.9.6",
        "drupal/devel": "4.*",
        "drupal/devel_a11y": "1.*",
        "drupal/devel_kint_extras": "1.0.0",
        "drupal/devel_php": "1.*",
        "drupal/drupal-extension": "3.*",
        "drupal/examples": "3.*",
        "drupal/pistachio": "1.*",
        "drupal/renderviz": "1.*",
        "drupal/search_kint": "1.*",
        "drupal/speedboxes": "1.*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "platform": {
            "php": "7.3"
        },
        "sort-packages": true
    },
    "scripts": {
        "post-create-project-cmd": [
            "bash ./scripts/init.sh"
        ],
        "post-install-cmd": [
            "bash ./scripts/update-libraries.sh"
        ],
        "post-update-cmd": [
            "bash ./scripts/update-libraries.sh"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "drupal-l10n": {
            "destination": "translations/contrib",
            "languages": [
                "fr"
            ]
        },
        "drupal-scaffold": {
            "file-mapping": {
                "[web-root]/INSTALL.txt": false,
                "[web-root]/README.txt": false,
                "[web-root]/example.gitignore": false,
                "[web-root]/modules/README.txt": false,
                "[web-root]/profiles/README.txt": false,
                "[web-root]/sites/README.txt": false,
                "[web-root]/themes/README.txt": false,
                "[web-root]/update.php": false,
                "[web-root]/web.config": false
            },
            "gitignore": false,
            "locations": {
                "web-root": "./www"
            }
        },
        "installer-paths": {
            "drush/Commands/{$name}": [
                "type:drupal-drush"
            ],
            "www/core": [
                "type:drupal-core"
            ],
            "www/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "www/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "www/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "www/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "www/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "www/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "www/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "merge-plugin": {
            "include": [
                "www/modules/contrib/charts/modules/charts_highcharts/composer.json",
                "www/modules/contrib/webform/composer.libraries.json",
                "www/modules/custom/*/composer.json",
                "www/profiles/custom/*/composer.json",
                "www/themes/custom/*/composer.json"
            ],
            "merge-extra": true,
            "merge-extra-deep": true,
            "merge-scripts": true
        },
        "patches": {
            "drupal/config_ignore": {
                "Support for export filtering via Drush": "https://www.drupal.org/files/issues/2020-01-11/support-for-export-2857247-44.patch"
            },
            "drupal/menu_attributes": {
                "Compatibility with fontawesome menu icons": "https://www.drupal.org/files/issues/menu_attributes-compatibility_fontawesome_menu_icons-2896652-2.patch"
            },
            "drupal/publication_date": {
                "Feeds support": "https://www.drupal.org/files/issues/2018-07-11/publication_date-feeds_support-2985180-2.patch"
            },
            "drupal/redis": {
                "Add status page and some statistics": "https://www.drupal.org/files/issues/redis-statistics-7.patch"
            },
            "drupal/views_infinite_scroll": {
                "Views Infinite Scroll does not work with EVA views": "https://www.drupal.org/files/issues/EVA_views_now_support_infinite_scroll.patch"
            }
        },
        "patchLevel": {
            "drupal/core": "-p2"
        }
    }
}
