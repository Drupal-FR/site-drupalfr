{
    "name": "drupal-fr/site-drupalfr",
    "description": "drupal.fr repository.",
    "type": "project",
    "homepage": "https://drupal.fr",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "Florent Torregrosa",
            "homepage": "https://drupal.fr/user/40695",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/Drupal-FR/site-drupalfr/issues",
        "source": "https://github.com/Drupal-FR/site-drupalfr",
        "docs": "https://github.com/Drupal-FR/site-drupalfr/blob/8.x-1.x/README.md"
    },
    "repositories": {
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    },
    "require": {
        "bower-asset/blazy": "1.8.2",
        "bower-asset/dropzone": "4.3.0",
        "bower-asset/fontawesome": "4.7.0",
        "bower-asset/fontawesome-iconpicker": "1.3.1",
        "bower-asset/jquery-simple-color": "1.2.2",
        "bower-asset/spectrum": "1.8.0",
        "composer/installers": "^1.0.21",
        "cweagans/composer-patches": "~1.0",
        "drupal-composer/drupal-l10n": "^1.0",
        "drupal-composer/drupal-scaffold": "^2.2",
        "drupal/admin_toolbar": "1.26.0",
        "drupal/blazy": "1.0.0-rc4",
        "drupal/block_field": "1.0.0-alpha8",
        "drupal/block_visibility_groups": "1.3.0",
        "drupal/color_field": "2.0.0",
        "drupal/commerce": "2.12.0",
        "drupal/commerce_license": "2.0.0-alpha18",
        "drupal/commerce_paypal": "1.0.0-beta2",
        "drupal/config_ignore": "2.1.0",
        "drupal/config_installer": "1.8.0",
        "drupal/config_split": "1.4.0",
        "drupal/content_moderation_notifications": "3.0.0-rc2",
        "drupal/core": "8.6.9",
        "drupal/dropzonejs": "2.0.0-alpha3",
        "drupal/drush_language": "1.0.0-rc2",
        "drupal/entity_browser": "2.1.0",
        "drupal/eu_cookie_compliance": "1.2.0",
        "drupal/eva": "1.3.0",
        "drupal/extra_field": "1.0.0",
        "drupal/facets": "1.2.0",
        "drupal/feeds": "3.0.0-alpha4",
        "drupal/field_formatter": "1.2.0",
        "drupal/field_group": "3.0.0-beta1",
        "drupal/field_permissions": "1.0.0-rc2",
        "drupal/flag": "4.0.0-alpha3",
        "drupal/focal_point": "1.0.0-beta6",
        "drupal/fontawesome": "1.4.0",
        "drupal/fontawesome_menu_icons": "1.5.0",
        "drupal/geofield": "1.1.0",
        "drupal/honeypot": "1.29.0",
        "drupal/inline_entity_form": "1.0.0-rc1",
        "drupal/leaflet": "1.2.0",
        "drupal/link_attributes": "1.5.0",
        "drupal/linked_field": "1.1.0",
        "drupal/mailchimp": "1.8.0",
        "drupal/media_entity_actions": "1.0.0-alpha2",
        "drupal/media_entity_browser": "2.0.0-alpha1",
        "drupal/menu_attributes": "1.x-dev#b3168e1e390d5d80c9c984ae8436f8b722e936c2",
        "drupal/metatag": "1.7.0",
        "drupal/migrate_plus": "4.1.0",
        "drupal/migrate_source_csv": "2.2.0",
        "drupal/migrate_tools": "4.1.0",
        "drupal/paragraphs": "1.5.0",
        "drupal/pathauto": "1.3.0",
        "drupal/publication_date": "2.0.0-beta1",
        "drupal/purge": "3.0.0-beta8",
        "drupal/r4032login": "1.1.0",
        "drupal/redirect": "1.3.0",
        "drupal/redis": "1.1.0",
        "drupal/search_api": "1.11.0",
        "drupal/search_api_solr": "2.6.0",
        "drupal/smart_trim": "1.1.0",
        "drupal/swiftmailer": "1.0.0-beta2",
        "drupal/varnish_purge": "1.11.0",
        "drupal/video_embed_field": "2.0.0",
        "drupal/views_block_filter_block": "1.0.0-beta1",
        "drupal/views_custom_cache_tag": "1.0.0",
        "drupal/views_infinite_scroll": "1.5.0",
        "drupal/webform": "5.1.0",
        "drupal/zurb_foundation": "6.0.0-alpha4",
        "drush/drush": "9.5.2",
        "oomphinc/composer-installers-extender": "^1.1",
        "wikimedia/composer-merge-plugin": "^1.4"
    },
    "require-dev": {
        "anam/phantomjs-linux-x86-binary": "~2.1.1",
        "bower-asset/d3": "3.5.*",
        "bower-asset/highlightjs": "9.12.*",
        "drupal/coder": "8.3.*",
        "drupal/config_inspector": "1.*",
        "drupal/console": "1.*",
        "drupal/devel": "2.*",
        "drupal/devel_a11y": "1.*",
        "drupal/devel_php": "1.*",
        "drupal/drupal-extension": "3.*",
        "drupal/examples": "1.*",
        "drupal/pistachio": "1.*",
        "drupal/renderviz": "1.*",
        "drupal/search_kint": "1.*",
        "drupal/speedboxes": "1.*",
        "webflo/drupal-core-require-dev": "~8.6"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "platform": {
            "php": "7.3"
        }
    },
    "scripts": {
        "post-create-project-cmd": [
            "bash ./scripts/init.sh"
        ],
        "post-install-cmd": [
            "@composer drupal:scaffold",
            "bash ./scripts/update-libraries.sh"
        ],
        "post-update-cmd": [
            "@composer drupal:scaffold",
            "bash ./scripts/update-libraries.sh"
        ]
    },
    "extra": {
        "drupal-l10n": {
            "destination": "translations/contrib",
            "languages": [
                "fr"
            ]
        },
        "drupal-scaffold": {
            "excludes": [
                ".ht.router.php",
                "update.php",
                "web.config",
                "sites/development.services.yml",
                "sites/example.settings.local.php"
            ]
        },
        "installer-paths": {
            "www/core": [
                "type:drupal-core"
            ],
            "www/modules/contrib/{$name}": [
                "type:drupal-module",
                "type:drupal-drush"
            ],
            "www/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "www/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "www/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "www/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "www/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "www/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ]
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "merge-plugin": {
            "include": [
                "www/modules/contrib/webform/composer.libraries.json",
                "www/modules/custom/*/composer.json",
                "www/profiles/custom/*/composer.json",
                "www/themes/custom/*/composer.json"
            ],
            "merge-extra": true,
            "merge-extra-deep": true,
            "merge-scripts": true
        },
        "patches": {
            "drupal/config_ignore": {
                "Support for export filtering via Drush": "https://www.drupal.org/files/issues/support_for_export-2857247-12.patch"
            },
            "drupal/flag": {
                "[regression] restore All option in flag Views filter handler": "https://www.drupal.org/files/issues/flag-all_option-2813503-3.patch"
            },
            "drupal/menu_attributes": {
                "Compatibility with fontawesome menu icons": "https://www.drupal.org/files/issues/menu_attributes-compatibility_fontawesome_menu_icons-2896652-2.patch"
            },
            "drupal/publication_date": {
                "Feeds support": "https://www.drupal.org/files/issues/2018-07-11/publication_date-feeds_support-2985180-2.patch"
            },
            "drupal/redis": {
                "Add status page and some statistics": "https://www.drupal.org/files/issues/redis-statistics-7.patch"
            },
            "drupal/views_infinite_scroll": {
                "Views Infinite Scroll does not work with EVA views": "https://www.drupal.org/files/issues/EVA_views_now_support_infinite_scroll.patch"
            }
        },
        "patchLevel": {
            "drupal/core": "-p2"
        }
    }
}
