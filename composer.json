    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "platform": {
            "php": "7.1"
        }
    },
    "scripts": {
        "drupal-l10n": "DrupalComposer\\DrupalL10n\\Plugin::download",
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "post-create-project-cmd": [
            "bash ./scripts/init.sh"
        ],
        "post-install-cmd": [
            "@drupal-scaffold",
            "bash ./scripts/update-libraries.sh"
        ],
        "post-update-cmd": [
            "@drupal-scaffold",
            "bash ./scripts/update-libraries.sh"
        ]
    },
    "extra": {
        "installer-paths": {
            "www/core": [
                "type:drupal-core"
            ],
            "www/modules/contrib/{$name}": [
                "type:drupal-module",
                "type:drupal-drush"
            ],
            "www/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "www/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "www/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "www/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "www/libraries/{$name}": [
                "type:drupal-library"
            ]
        },
        "drupal-scaffold": {
            "excludes": [
                ".ht.router.php",
                "update.php",
                "web.config",
                "sites/development.services.yml",
                "sites/example.settings.local.php"
            ]
        },
        "drupal-l10n": {
            "destination": "translations/contrib",
            "languages": [
                "fr"
            ]
        },
        "patches": {
            "drupal/color_field": {
                "Color Boxes Widget default values dont work": "https://www.drupal.org/files/issues/color_field-2854199-7.patch"
            },
            "drupal/config_ignore": {
                "Support for export filtering via Drush": "https://www.drupal.org/files/issues/support_for_export-2857247-12.patch"
            },
            "drupal/config_inspector": {
                "Filter entities with schema errors": "https://www.drupal.org/files/issues/filter_entities_with-2843729-7.patch"
            },
            "drupal/flag": {
                "[regression] restore All option in flag Views filter handler": "https://www.drupal.org/files/issues/flag-all_option-2813503-3.patch"
            },
            "drupal/menu_attributes": {
                "Compatibility with fontawesome menu icons": "https://www.drupal.org/files/issues/menu_attributes-compatibility_fontawesome_menu_icons-2896652-2.patch"
            },
            "drupal/redis": {
                "Add status page and some statistics": "https://www.drupal.org/files/issues/redis-statistics-7.patch"
            },
            "drupal/views_infinite_scroll": {
                "Views Infinite Scroll does not work with EVA views": "https://www.drupal.org/files/issues/EVA_views_now_support_infinite_scroll.patch"
            }
        }
    }
}
