        "bower-asset/jquery-simple-color": "1.2.2",
        "bower-asset/spectrum": "1.8.0",
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "~1.0",
        "drupal-composer/drupal-l10n": "^2.0",
        "drupal/address": "1.9.0",
        "drupal/admin_toolbar": "3.1.0",
        "drupal/advancedqueue": "1.0.0-rc3",
        "drupal/blazy": "2.12.0",
        "drupal/block_field": "1.0.0-rc2",
        "drupal/block_visibility_groups": "1.4.0",
        "drupal/charts": "3.3.0",
        "drupal/chosen": "2.10.0",
        "drupal/color_field": "2.5.0",
        "drupal/commerce": "2.30.0",
        "drupal/commerce_license": "2.0.0-beta1",
        "drupal/commerce_paypal": "1.0.0-rc6",
        "drupal/commerce_recurring": "1.0.0-beta6",
        "drupal/config_ignore": "2.3.0",
        "drupal/config_split": "1.8.0",
        "drupal/content_moderation_notifications": "3.4.0",
        "drupal/core-recommended": "9.3.14",
        "drupal/crop": "2.2.0",
        "drupal/csv_serialization": "2.0.0",
        "drupal/ctools": "3.7.0",
        "drupal/dropzonejs": "2.6.0",
        "drupal/drush_language": "1.0.0-rc4",
        "drupal/entity": "1.2.0",
        "drupal/entity_browser": "2.6.0",
        "drupal/entity_reference_revisions": "1.9.0",
        "drupal/eu_cookie_compliance": "1.19.0",
        "drupal/eva": "2.1.0",
        "drupal/extra_field": "1.3.0",
        "drupal/facets": "2.0.2",
        "drupal/feeds": "3.0.0-beta1",
        "drupal/field_formatter": "3.x-dev@dev",
        "drupal/field_group": "3.2.0",
        "drupal/field_permissions": "1.1.0",
        "drupal/flag": "4.0.0-beta2",
        "drupal/focal_point": "1.5.0",
        "drupal/fontawesome": "2.23.0",
        "drupal/fontawesome_menu_icons": "1.9.0",
        "drupal/geofield": "1.39.0",
        "drupal/gin": "3.0.0-beta3",
        "drupal/honeypot": "2.1.0",
        "drupal/imageapi_optimize": "4.0.0",
        "drupal/imageapi_optimize_resmushit": "2.0.0-beta1",
        "drupal/inline_entity_form": "1.0.0-rc11",
        "drupal/interval": "1.9.0",
        "drupal/leaflet": "2.2.2",
        "drupal/libraries": "3.0.0-beta2",
        "drupal/link_attributes": "1.11.0",
        "drupal/linked_field": "1.3.0",
        "drupal/mailsystem": "4.3.0",
        "drupal/menu_link_attributes": "1.2",
        "drupal/metatag": "1.19.0",
        "drupal/migrate_plus": "5.3.0",
        "drupal/migrate_source_csv": "3.5.0",
        "drupal/migrate_tools": "5.1.0",
        "drupal/paragraphs": "1.14.0",
        "drupal/pathauto": "1.10.0",
        "drupal/profile": "1.4.0",
        "drupal/publication_date": "2.0.0-beta4",
        "drupal/r4032login": "2.1.0",
        "drupal/recurring_period": "1.0.0",
        "drupal/redirect": "1.7.0",
        "drupal/redis": "1.5.0",
        "drupal/search_api": "1.23.0",
        "drupal/smart_trim": "1.3.0",
        "drupal/state_machine": "1.6.0",
        "drupal/swiftmailer": "2.2.0",
        "drupal/token": "1.10.0",
        "drupal/video_embed_field": "2.4.0",
        "drupal/views_block_filter_block": "1.2.0",
        "drupal/views_bulk_edit": "2.6.0",
        "drupal/views_bulk_operations": "4.1.2",
        "drupal/views_custom_cache_tag": "1.2.0",
        "drupal/views_data_export": "1.1.0",
        "drupal/views_field_view": "1.0.0-beta3",
        "drupal/views_infinite_scroll": "2.0.0",
        "drupal/webform": "6.1.3",
        "drupal/zurb_foundation": "6.0.0-alpha6",
        "drush/drush": "11.0.9",
        "oomphinc/composer-installers-extender": "^2",
        "vlucas/phpdotenv": "5.*",
        "webflo/drupal-finder": "1.*",
        "wikimedia/composer-merge-plugin": "^2"
    },
    "require-dev": {
        "anam/phantomjs-linux-x86-binary": "~2.1.1",
        "bower-asset/d3": "3.5.*",
        "bower-asset/highlightjs": "9.12.*",
        "drupal-composer/drupal-security-advisories": "9.x-dev",
        "drupal/coder": "8.3.*",
        "drupal/config_devel": "1.*",
        "drupal/config_inspector": "2.*",
        "drupal/console": "1.*",
        "drupal/core-dev": "9.3.12",
        "drupal/devel": "4.*",
        "drupal/devel_a11y": "1.*",
        "drupal/devel_kint_extras": "1.0.0",
        "drupal/devel_php": "1.*",
        "drupal/drupal-extension": "4.*",
        "drupal/examples": "3.*",
        "drupal/renderviz": "1.*",
        "drupal/speedboxes": "1.*",
        "drupal/upgrade_status": "3.*",
        "drupol/phpcsfixer-configs-drupal": "2.*",
        "mglaman/drupal-check": "^1.1",
        "mglaman/phpstan-drupal": "1.*",
        "mortenson/psalm-plugin-drupal": "dev-master",
        "palantirnet/drupal-rector": "0.*",
        "phpmd/phpmd": "2.*",
        "phpro/grumphp": "1.*",
        "phpspec/prophecy-phpunit": "2.*",
        "phpstan/extension-installer": "1.*",
        "phpstan/phpstan-deprecation-rules": "1.*",
        "phpstan/phpstan-phpunit": "1.*",
        "povils/phpmnd": "2.*",
        "roave/security-advisories": "dev-master",
        "sebastian/phpcpd": "6.*"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": [
            "load.environment.php"
        ]
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "composer/package-versions-deprecated": true,
            "cweagans/composer-patches": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "drupal-composer/drupal-l10n": true,
            "drupal-composer/drupal-paranoia": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-vendor-hardening": true,
            "oomphinc/composer-installers-extender": true,
            "phpro/grumphp": true,
            "phpstan/extension-installer": true,
            "wikimedia/composer-merge-plugin": true,
            "zaporylie/composer-drupal-optimizations": true,
            "drupal/console-extend-plugin": true
        },
        "discard-changes": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "8.0"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "drupal-l10n": {
            "destination": "translations/contrib",
            "languages": [
                "fr"
            ]
        },
        "drupal-paranoia": {
            "app-dir": "app",
            "excludes": [
                "core/install.php",
                "core/rebuild.php",
                "core/modules/statistics/statistics.php"
            ],
            "web-dir": "www"
        },
        "drupal-scaffold": {
            "file-mapping": {
                "[project-root]/.gitattributes": false,
                "[web-root]/.csslintrc": false,
                "[web-root]/.eslintrc.json": false,
                "[web-root]/INSTALL.txt": false,
                "[web-root]/README.md": false,
                "[web-root]/example.gitignore": false,
                "[web-root]/modules/README.txt": false,
                "[web-root]/profiles/README.txt": false,
                "[web-root]/sites/README.txt": false,
                "[web-root]/themes/README.txt": false,
                "[web-root]/update.php": false,
                "[web-root]/web.config": false
            },
            "gitignore": false,
            "locations": {
                "web-root": "./app"
            }
        },
        "grumphp": {
            "config-default-path": "scripts/quality/grumphp/grumphp.yml"
        },
        "installer-paths": {
            "app/core": [
                "type:drupal-core"
            ],
            "app/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "app/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "app/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "app/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "app/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "app/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "app/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "merge-plugin": {
            "include": [
                "app/modules/custom/*/composer.json",
                "app/profiles/custom/*/composer.json",
                "app/themes/custom/*/composer.json"
            ],
            "merge-extra": true,
            "merge-extra-deep": true,
            "merge-scripts": true
        },
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "drupal/coder": {
                "Add a rule on block machine name - https://www.drupal.org/project/coder/issues/3061184#comment-14062494": "https://www.drupal.org/files/issues/2021-04-16/coder-block_machine_name-3061184-9.patch"
            },
            "drupal/config_ignore": {
                "Support for export filtering via Drush": "https://www.drupal.org/files/issues/2020-01-11/support-for-export-2857247-44.patch"
            },
            "drupal/core": {
                "1 - BrowserTestBase::setUp() must be compatible with PHPUnit\\Framework\\TestCase::setUp() - https://www.drupal.org/project/drupal/issues/3182103#comment-14273323": "https://www.drupal.org/files/issues/2021-10-28/3182103-25-void-and-protected.patch"
            },
            "drupal/devel": {
                "Brings back the Available Methods & Properties tabs for Kint dumps — Issue #221": "https://raw.githubusercontent.com/politsin/snipets/master/patch/kint.patch"
            },
            "drupal/menu_attributes": {
                "Compatibility with fontawesome menu icons": "https://www.drupal.org/files/issues/menu_attributes-compatibility_fontawesome_menu_icons-2896652-2.patch"
            },
            "drupal/publication_date": {
                "Feeds support": "https://www.drupal.org/files/issues/2018-07-11/publication_date-feeds_support-2985180-2.patch"
            },
            "drupal/redis": {
                "Add status page and some statistics": "https://www.drupal.org/files/issues/redis-statistics-7.patch"
            },
            "drupal/views_infinite_scroll": {
                "Views Infinite Scroll does not work with EVA views": "https://www.drupal.org/files/issues/EVA_views_now_support_infinite_scroll.patch"
            }
        }
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    }
}
