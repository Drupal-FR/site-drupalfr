        "bower-asset/spectrum": "1.8.0",
        "composer/installers": "^1.7.0",
        "cweagans/composer-patches": "~1.0",
        "drupal-composer/drupal-l10n": "^2.0",
        "drupal/address": "1.8.0",
        "drupal/admin_toolbar": "2.3.0",
