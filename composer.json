        "drupal/commerce_paypal": "1.0.0-beta11",
        "drupal/config_ignore": "2.2.0",
        "drupal/config_split": "1.6.0",
        "drupal/content_moderation_notifications": "3.2.0",
        "drupal/core-recommended": "8.9.6",
        "drupal/crop": "2.1.0",
        "drupal/csv_serialization": "1.4.0",
        "drupal/ctools": "3.4.0",
        "drupal/dropzonejs": "2.3.0",
        "drupal/drush_language": "1.0.0-rc3",
        "drupal/entity": "1.1.0",
