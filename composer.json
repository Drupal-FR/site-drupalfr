        "drupal/facets": "1.5.0",
        "drupal/feeds": "3.0.0-alpha9",
        "drupal/field_formatter": "1.2.0",
        "drupal/field_group": "3.1.0",
        "drupal/field_permissions": "1.1.0",
        "drupal/flag": "4.0.0-beta2",
        "drupal/focal_point": "1.5.0",
        "drupal/fontawesome": "1.4.0",
        "drupal/fontawesome_menu_icons": "1.5.0",
        "drupal/geofield": "1.15.0",
        "drupal/honeypot": "1.30.0",
        "drupal/imageapi_optimize": "2.0.0-beta1",
        "drupal/imageapi_optimize_resmushit": "1.0.0-beta1",
        "drupal/inline_entity_form": "1.0.0-rc7",
        "drupal/interval": "1.7.0",
